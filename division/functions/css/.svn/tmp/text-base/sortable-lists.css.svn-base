.isSortable .sortItem { background: #EEE;border: 1px solid #d0d0d0;-moz-border-radius: 8px;-webkit-border-radius: 8px;border-radius: 8px;margin: 0 0 3px;clear: both;}
.isSortable td{padding:10px;}
.menu-item .sortItem .handle div,
.slide-item .sortItem .handle div {float: left;width: 24px;height: 24px;background: transparent url("../images/menu-move.png") no-repeat 0 0;margin: 0 0 0 0.25em;}
